<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:c="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:core="urn:import:com.sencha.gxt.widget.core.client" 
	xmlns:f="urn:import:com.sencha.gxt.widget.core.client.form"
	xmlns:b="urn:import:com.sencha.gxt.widget.core.client.button">
	<core:FramedPanel width="300" headingText="笔数为空值时为单笔并不清空">
		<c:VerticalLayoutContainer>
			<f:FieldLabel text="笔数">
				<f:widget>
					<f:IntegerField ui:field="number" />
				</f:widget>
			</f:FieldLabel>
			<f:FieldLabel text="key长度">
				<f:widget>
					<f:IntegerField ui:field="keyLength" />
				</f:widget>
			</f:FieldLabel>
			<f:FieldLabel text="value长度">
				<f:widget>
					<f:IntegerField ui:field="valueLength" />
				</f:widget>
			</f:FieldLabel>
		</c:VerticalLayoutContainer>
		<core:button>
			<b:TextButton ui:field="save" text="保存"></b:TextButton>
		</core:button>
	</core:FramedPanel>
</ui:UiBinder> 